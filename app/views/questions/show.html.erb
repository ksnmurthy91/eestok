<div class="page-header">
  <%= link_to questions_path, class: 'btn btn-default' do %>
    All Questions
  <% end %>
  <%= link_to edit_question_path(@question), class: 'btn btn-primary' do %>
    Edit
  <% end %>
  <h1>Show question</h1>
</div>

<dl class="dl-horizontal">
  <dt>Description:</dt>
  <dd><%= @question.description %></dd>

  <dt>User:</dt>
  <dd><%= @question.user %></dd>

</dl>
